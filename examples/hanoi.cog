Def Move discs as (

	Let N be number of discs;
	Let A be starting location;
	Let B be temporary location;
	Let C be ending location;

	Do If (== 0 N) then () else (

		Move - 1 N discs from A via C to B;

		Prints ('Move dics ' N ' from ' A ' to ' C);

		Move - 1 N discs from B via A to C;
	);
);

