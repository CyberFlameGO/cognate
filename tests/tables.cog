Drop Table (\hello is 'world', \foo is 'bar');

Print 'PASS: Creating table without error';

Print If (Table (\hello is 'world', \foo is 'bar'), == Table (\hello is 'world', \foo is 'bar'))
  'PASS: Comparing identical tables'
  'FAIL: Comparing identical tables';

Print If (Table (\hello is 'world', \foo is 'bar'), /= Table (\hello is 'bye', \foo is 'baz'))
  'PASS: Comparing different tables'
  'FAIL: Comparing different tables';

Print If (Insert \foo is 'bar' into Table (\a is \b), == Table (\a is \b, \foo is 'bar'))
  'PASS: Inserting value into table'
  'FAIL: Inserting value into table';

Print If (Get \foo from Table (\foo is 12), == 12)
  'PASS: Getting value from table'
  'FAIL: Getting value from table';
