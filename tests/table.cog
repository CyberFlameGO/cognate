Let Tab be Table
(
  'Hello' is 'world';
  'foo'   is 'bar';
);

Print 'PASS: Creating table without error.';

If (Get 'Hello' from Tab, == 'world') then (
  Print 'PASS: Extracting key from small table.'
);
Else (
  Print 'FAIL: Extracting key from small table.'
);

Insert 'bar' is 'baz' into Tab, Set Tab;

Print 'PASS: Inserting into table without error.';

If (Get 'bar' from Tab, == 'baz') then (
  Print 'PASS: Extracting inserted key from table.'
);
Else (
  Print 'FAIL: Extracting inserted key from table.'
);

If (Values in Table ('hello' is 'world'), == List ('world')) then (
  Print 'PASS: Extracting values from small table.'
);
Else (
  Print 'PASS: Extracting values from small table.'
);

Let Tab2 be
Table (
  'foo' is 'bar',
  'Hello' is 'world'
);

Let Tab3 be
Table (
  'foo' is 'baz'
  'Hello' is 'world'
);

If (Tab, == Tab2) then (
  Print 'PASS: Comparing identical tables.'
);
Else (
  Print 'FAIL: Comparing identical tables.'
);

If (Tab, != Tab3) then (
  Print 'PASS: Comparing different tables.'
);
Else (
  Print 'FAIL: Comparing different tables.'
);
