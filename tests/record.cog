Record Result is
(
  Success (String?)
);

make Result from 'PASS: Accessing record field';
Print Success;

Record Tuple is
(
  First, Second;
);

If (Tuple 1 2, == Tuple 1 2)
   (Print 'PASS: Comparing identical records')
   (Print 'FAIL: Comparing identical records');

If (Tuple 1 2, != Tuple 3 4)
   (Print 'PASS: Comparing non-identical records')
   (Print 'FAIL: Comparing non-identical records');
