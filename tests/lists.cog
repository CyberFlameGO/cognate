Let Foo be List (1,2,3);

If (Foo, == Foo) (
  Print 'PASS: Comparing equal lists'
);
Else (
  Print 'FAIL: Comparing equal lists'
);

If (Foo, != List (5,6,7)) (
  Print 'PASS: Comparing unequal lists'
);
Else (
  Print 'FAIL: Comparing unequal lists'
);

If (List (), == List ()) (
  Print 'PASS: Comparing two empty lists'
);
Else (
  Print 'FAIL: Comparing two empty lists'
);

If (List (), != Foo) (
  Print 'PASS: An empty list to a non-empty list'
);
Else (
  Print 'FAIL: An empty list to a non-empty list'
);

If (Push 42 to Foo, == List (42, 1, 2, 3)) (
  Print 'PASS: Pushing object to list'
);
Else (
  Print 'FAIL: Pushing object to list'
);

If (Head of Foo, == 1) (
  Print 'PASS: Getting list\'s Head'
);
Else (
  Print 'FAIL: Getting list\'s Head'
);

If (Head of List (1), == 1) (
  Print 'PASS: Getting Head of single-element list'
);
Else (
  Print 'FAIL: Getting Head of single-element list'
);

If (Tail of Foo, == List (2, 3)) (
  Print 'PASS: Getting list\'s Tail'
);
Else (
  Print 'FAIL: Getting list\'s Tail'
);

If (Tail of List (1), == List ()) (
  Print 'PASS: Getting Tail of single-element list'
);
Else (
  Print 'FAIL: Getting Tail of single-element list'
);
